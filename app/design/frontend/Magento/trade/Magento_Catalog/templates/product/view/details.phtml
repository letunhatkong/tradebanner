require(['jquery', 'jquery/ui'], function ($) {
    $(document).ready(function () {

        setTimeout(function(){
            $(".product-custom-option > option").each(function(index, item) {   
                var op = $(item);
                var title = op.text();

                if (title.indexOf('$')) {
                    var ar = title.split('+$');
                    title = ar[0];
                }              
                $(this).text(title);

            })      
        }, 1000);       
    });
});
</script>